

    <div style="background-color: aliceblue;">

        <table class="table">
            <thead>
                <tr>
                    <th scope="col">stt</th>
                    <th scope="col">tên video</th>
                    <th scope="col">id_youtube</th>
                    <th scope="col">hành động</th>

                </tr>
            </thead>
            <tbody>
                {{#each videos}}
                <tr>
                    <th>{{add 1 @index}}</th>
                    <td>
                        <ul>
                            {{stt}}
                        </ul>
                    </td>
                    <td>{{youtube_id}}</td>

                    <td>
                        <a type="button" href="/admins/{{slug}}/{{_id}}/editvideo" class="btn btn-outline-primary">Sửa
                            video</a>
                            <form action="/admins/{{slug}}/{{_id}}/deletevideo" method="POST">

                            <button type="submit" class="btn btn-outline-danger" >Xóa video</button>
                            </form>
                        {{/each}}
                    </td>
                </tr>
            </tbody>
        </table>
    </div>

</div>



